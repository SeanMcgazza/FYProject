<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.example.sean.fyapp.Main2Activity" android:background="#670606">

    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data Base"
            android:id="@+id/DB"
            android:layout_above="@+id/Back" android:layout_alignParentStart="true"
            android:layout_marginBottom="103dp" android:layout_alignEnd="@+id/Back"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back"
            android:id="@+id/Back"
            android:layout_alignParentBottom="true" android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Exercises"
            android:id="@+id/Exercises"
            android:layout_centerVertical="true" android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/DB"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upload EMG Exercise"
            android:id="@+id/Upload"
            android:layout_alignParentTop="true" android:layout_alignParentStart="true"
            android:layout_marginTop="87dp" android:layout_alignEnd="@+id/Exercises"/>
</RelativeLayout>
