<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.example.sean.fyapp.EMG_Layout" android:background="#670606">

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Front Lower"
            android:id="@+id/F_lower"
            android:layout_alignParentBottom="true" android:layout_alignParentStart="true"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Front Upper"
            android:id="@+id/F_upper" android:layout_above="@+id/F_lower" android:layout_alignParentStart="true"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back Upper"
            android:id="@+id/B_upper" android:layout_alignTop="@+id/F_upper" android:layout_toEndOf="@+id/F_lower"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back Lower"
            android:id="@+id/B_lower" android:layout_below="@+id/B_upper" android:layout_centerHorizontal="true"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
</RelativeLayout>
